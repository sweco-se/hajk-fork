    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.2",
    "helmet": "^4.6.0",
    "http-proxy-middleware": "^2.0.1",
    "log4js": "^6.3.0",
    "node-windows": "^1.0.0-beta.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/register": "^7.16.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6"
  },
  "author": "Jacob Wodzy≈Ñski <jacob.wodzynski@halmstad.se> (https://github.com/hajkmap)"
